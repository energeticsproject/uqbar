{
  "name": "@energetics/uqbar",
  "version": "0.1.0",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "types": "build/index.d.ts",
  "scripts": {
    "dev": "nodemon",
    "postinstall": "cd dev && npm install",
    "prepare": "node build.js",
    "start": "node -r dotenv/config build/index.node.js",
    "test": "mocha test/index.js --watch --watch-files build,test"
  },
  "nodemonConfig": {
    "exec": "npm run prepare && npm start 2>&1 | tee out.log",
    "watch": [
      "src",
      "dev-cm/src",
      "build.js",
      ".env"
    ],
    "ext": "js,ts,tsx,html,grammar,env"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^0.19.8",
    "@codemirror/closebrackets": "^0.19.0",
    "@codemirror/commands": "^0.19.5",
    "@codemirror/comment": "^0.19.0",
    "@codemirror/fold": "^0.19.1",
    "@codemirror/highlight": "^0.19.6",
    "@codemirror/history": "^0.19.0",
    "@codemirror/language": "^0.19.5",
    "@codemirror/lint": "^0.19.3",
    "@codemirror/matchbrackets": "^0.19.3",
    "@codemirror/rectangular-selection": "^0.19.1",
    "@codemirror/search": "^0.19.2",
    "@codemirror/state": "^0.19.5",
    "@codemirror/view": "^0.19.19",
    "@lezer/common": "^0.15.7",
    "@lezer/generator": "^0.15.2",
    "@lezer/lr": "^0.15.4",
    "@types/node": "^16.11.10",
    "buffer": "^6.0.3",
    "dotenv": "^10.0.0",
    "esbuild-wasm": "0.13.14",
    "sha.js": "^2.4.11",
    "uninstall": "^0.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.11",
    "@types/sha.js": "^2.4.0",
    "esbuild": "0.13.14",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "4.4.3"
  }
}
